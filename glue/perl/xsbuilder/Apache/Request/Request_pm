use strict;
use warnings FATAL => 'all';
use Apache2;
use APR;
use APR::Table;

package Apache::Upload::Table;
use base 'APR::Table';

package Apache::Request::Table;
use base 'APR::Table';

package Apache::Request;
push our(@ISA), __PACKAGE__ -> env;

sub config {}
sub new {
    my $class = shift;
    my $env = shift;
    my $req = $class->request($env);
    $req->config(@_);
    return $req;
}
